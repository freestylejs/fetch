{
    "name": "@metal-box/openapi-generator",
    "version": "0.0.1",
    "description": "Generate metal-box fetch client from OpenAPI spec",
    "author": "danpacho",
    "license": "MIT",
    "keywords": [
        "openapi",
        "generator",
        "fetch"
    ],
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "metal-openapi-gen": "dist/index.js"
    },
    "scripts": {
        "build": "tsup src/index.ts --format=cjs,esm --dts",
        "build:fast": "tsup src/index.ts --format=cjs,esm",
        "dev": "pnpm build --watch",
        "test": "vitest"
    },
    "dependencies": {
        "commander": "^12.1.0",
        "fs-extra": "^11.2.0",
        "prettier": "^3.3.2",
        "openapi-types": "^12.1.3",
        "lodash": "^4.17.21",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.14.2",
        "@types/prettier": "^3.0.0",
        "@types/lodash": "^4.17.5",
        "vitest": "^1.6.0",
        "ts-expect": "^1.3.0"
    }
}
